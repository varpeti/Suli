PROGRAM f
  VÁLTOZÓK:
    x, i, j: EGÉSZ,
    gy: VALÓS
 
  BE: x
  CIKLUS AMÍG (x MOD 2 = 0)
    KI: 2, " "
    x := x DIV 2
  CIKLUS_VÉGE
  CIKLUS AMÍG (x MOD 3 = 0)
    KI: 3, " "
    x := x DIV 3
  CIKLUS_VÉGE
  gy := x ^ (0.5)
  i := 5
  j := 2
  CIKLUS AMÍG i <= gy ÉS x > 1
    CIKLUS AMÍG (x MOD i = 0)
      KI: i, " "
      x := x DIV i
      gy := x ^ (0.5)
    CIKLUS_VÉGE
    i := i + j
    j := 6 - j
  CIKLUS_VÉGE
  HA x > 1 AKKOR
    KI: x
  HA_VÉGE
PROGRAM_VÉGE
